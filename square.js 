<meta charset = "utf-8">
<title> Square Function </title>
<script> 
document.writeln( "<h1> Square the numbers from 1 to 25 <h2>" );

for ( var x = 1; x <= 25; ++x )
document.writeln("<p> The square of " + x + " is " + 
square( x ) + "</p>" );
function square ( y )  
{
 return y * y ;
}
</script>
